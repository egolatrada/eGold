modules = ["python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["discord:1.0.0", "javascript_openai_ai_integrations:1.0.0", "javascript_gemini_ai_integrations:1.0.0", "javascript_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["u-root-cmds"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev-bot-replit"

[[workflows.workflow]]
name = "dev-bot-replit"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node watchdog.js"

[workflows.workflow.metadata]
outputType = "console"

[deployment]
deploymentTarget = "gce"
run = ["bash", "-c", "node index.js & cd verification-bot && node index.js"]
ignorePorts = true
